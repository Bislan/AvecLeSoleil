<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contact" table="contact">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="45" nullable="false"/>
    <field name="message" type="text" column="message" nullable="false"/>
    <field name="adress" type="string" column="adress" length="45" nullable="true"/>
    <field name="email" type="string" column="email" length="45" nullable="true"/>
    <field name="fax" type="integer" column="fax" nullable="true"/>
    <field name="tel" type="integer" column="tel" nullable="true"/>
  </entity>
</doctrine-mapping>
